{
  "name": "GitHub Notifications Alert",
  "description": "Add a favicon alert to GitHub Notifications",
  "version": "0.1",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/notifications/beta"
      ],
      "js": [
        "extension.js"
      ]
    }
  ]
}
